{
  "name": "Q-it cookie detector",
  "manifest_version": 3,
  "version": "1.0",
  "description": "Detects what cookies are installed on a domain | heam@queue-it.com",
  "permissions": ["cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
